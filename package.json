{
  "name": "fsa-models",
  "version": "0.0.1",
  "private": false,
  "description": "Strict-typed fsa models",
  "main": "./lib/create-model.js",
  "typings": "./lib/create-model.d.ts",
  "files": [
    "lib"
  ],
  "author": "Igor Emets <ya.peren@yandex.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOuser/fsa-models"
  },
  "license": "WTFPL",
  "scripts": {
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "test": "jest",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "prettier": "prettier --write *.js **/*.js **/*.ts **/*.tsx",
    "clean": "rm -rf lib",
    "release": "standard-version",
    "prepack": "npm run clean && npm run build",
    "precommit": "lint-staged",
    "cm": "git-cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
        "npm run prettier",
        "git add"
    ]
  },
  "dependencies": {
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0",
    "tslib": "^1.9.3",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^1.1.0",
    "typescript-fsa-redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@types/node": "^10.12.10",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^5.9.0",
    "eslint-plugin-prettier": "^3.0.0",
    "git-cz": "^1.7.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "vrsource-tslint-rules": "^6.0.0",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
